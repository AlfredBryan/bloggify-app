    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.6",
    "mongoose-unique-validator": "^2.0.2",
    "multer": "^1.4.1",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.4"
  }
